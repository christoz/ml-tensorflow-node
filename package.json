{
  "name": "ml-tensorflow-node",
  "version": "1.0.0",
  "description": "image classification with tensorflow-node js",
  "repository": "git@github.com:christoz/ml-tensorflow-node.git",
  "author": "Christos Ziogas <cziogas@steficon.gr>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d dist -s & next build ./client",
    "debug": "babel-node --inspect-brk=9229 ./src/index.js",
    "dev": "nodemon src/index.js | pino-pretty -c -t & next dev ./client -p 9070",
    "lint": "eslint --fix src",
    "start": "node dist/index.js & next start ./client -p 9070",
    "createLabels": "node src/domain/classification/createJSONLabels.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "execMap": {
      "js": "babel-node"
    }
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/multer": "^3.0.0",
    "@tensorflow/tfjs-node": "^2.0.1",
    "archiver": "^4.0.1",
    "boom": "^7.3.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "extract-zip": "^2.0.1",
    "glob": "^7.1.6",
    "koa": "^2.13.0",
    "koa-body-parser": "^1.1.2",
    "koa-bodyparser": "^4.3.0",
    "koa-pino-logger": "^3.0.0",
    "koa-router": "^9.0.1",
    "koa-static": "^5.0.0",
    "multer": "^1.4.2",
    "next": "^11.1.3",
    "pino": "^6.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.9"
  }
}
